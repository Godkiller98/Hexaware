
data=[10,20,30,45,50,40,69];
print(data);
print(type(data));

print(data[2:4]);
data.append(200);
print("max : ",max(data))
print("min : ",min(data))
print("Length : ",len(data))
print(data);
print("Pop : ",data.pop())

data.insert(3,'A');
print(data)

data.remove(50);
print(data)

data.reverse();
print(data);

data=[10,20,30,40,50];
total=0;
for i in data:
    total=total+i;

print(total);
